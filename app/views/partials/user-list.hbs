<div class="uk-margin uk-width-expand uk-margin-auto uk-card uk-card-default uk-card-body uk-box-shadow-large">
  <table class="uk-table">
    <caption>
      Users
    </caption>
    <thead>
      <th>Name</th>
      <th>Delete</th>
    </thead>
    <tbody class="uk-text-left">
      {{#each users}}
      <tr>       
        <td> {{firstName}} {{lastName}} </td>        
        <td><a href="/deleteuser/{{_id}}" class="uk-card uk-card-small uk-card-hover uk-padding-small"><i class="far fa-trash-alt fa-2x" style="color:rgb(92, 73, 79)" title="Delete {{firstName}} {{lastName}}" pos="bottom" uk-tooltip></i></a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>